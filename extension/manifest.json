{
    "name": "Face rek extension",
    "version": "1.0",
    "description": "Rek face",
    "permissions": ["declarativeContent", "storage", "videoCapture"],
    "background": {
        "scripts": ["background.js", "jquery.min.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": true,
        "open_in_tab": true
    },
    "options_page": "options.html",
    "manifest_version": 2,
    "web_accessible_resources": ["logo.png"]
}
